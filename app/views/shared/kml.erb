<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
<Document>
  <%= render 'shared/kml_styles' %>

  <% @segments.each do |segment| %>
    <Placemark>
      <name><%= segment.id %></name>
      <styleUrl>#<%= segment.style_id %></styleUrl>
      <%= raw segment.kml %>
    </Placemark>
  <% end %>
</Document>
</kml>
