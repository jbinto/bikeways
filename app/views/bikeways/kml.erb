<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
<Document>
  <Style id="blueLine">
    <LineStyle>
      <color>ffff0000</color>
      <width>4</width>
    </LineStyle>
  </Style>

  <% @bikeway.bikeway_segments.each do |segment| %>
    <Placemark>
      <name><%= segment.id %></name>
      <styleUrl>#blueLine</styleUrl>
      <%= raw segment.kml %>
    </Placemark>
  <% end %>
</Document>
</kml>
